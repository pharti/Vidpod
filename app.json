{
  "expo": {
    "name": "YidPod",
    "slug": "spot-mobile",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yidpod",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yaschaffel.spotmobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.yaschaffel.spotmobile"
    }
  }
}
